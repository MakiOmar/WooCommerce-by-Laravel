{
    "name": "your-vendor/woo-order-dashboard",
    "description": "A Laravel package for managing WooCommerce orders with advanced filtering capabilities",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "YourVendor\\WooOrderDashboard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YourVendor\\WooOrderDashboard\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YourVendor\\WooOrderDashboard\\WooOrderDashboardServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
} 