{
    "name": "makiomar/woo-order-dashboard",
    "description": "A Laravel package for displaying WooCommerce orders in your Laravel admin panel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammad Omar",
            "email": "maki3omar@gmail.com"
        }
    ],
    "require": {
    "php": "^8.1",
    "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
    "illuminate/database": "^9.0|^10.0|^11.0|^12.0",
    "illuminate/view": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
    "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0",
    "phpunit/phpunit": "^9.0|^10.0"
    },

    "autoload": {
        "psr-4": {
            "Makiomar\\WooOrderDashboard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Makiomar\\WooOrderDashboard\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Makiomar\\WooOrderDashboard\\WooOrderDashboardServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
} 